{
  "manifest_version": 3,
  "name": "Lean Meeting Insights Pro",
  "version": "2.3.12",
  "description": "Live transcript + local LLM meeting insights (Ollama).",
  "action": {
    "default_title": "Lean Meeting Insights Pro"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:11434/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}